{
    "azure.showSignedInEmail": true,
    "azureTerraform.test.location": "eastus",
    "breadcrumbs.enabled": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.folding": true,
    "editor.foldingStrategy": "indentation",
    "editor.fontSize": 14,
    "editor.formatOnSave": true,
    "editor.glyphMargin": false,
    "editor.insertSpaces": false,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.showFoldingControls": "always",
    "editor.tabCompletion": "on",
    "editor.trimAutoWhitespace": true,
    "editor.wordWrap": "on",
    "explorer.compactFolders": false,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.alwaysSignOff": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.detectSubmodulesLimit": 100,
    "git.enableCommitSigning": true,
    "git.enableSmartCommit": true,
    "git.fetchOnPull": true,
    "git.pruneOnFetch": true,
    "git.pullTags": false,
    "git.supportCancellation": true,
    "hadolint.hadolintPath": "/usr/local/bin/hadolint",
    "json.format.enable": true,
    "markdown.preview.breaks": true,
    "markdown.preview.scrollEditorWithPreview": true,
    "redhat.telemetry.enabled": false,
    "scm.alwaysShowRepositories": true,
    "scm.defaultViewMode": "tree",
    "scm.providerCountBadge": "visible",
    "scm.repositories.visible": 0,
    "search.collapseResults": "auto",
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "search.useIgnoreFiles": false,
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "settingsSync.ignoredExtensions": [],
    "settingsSync.ignoredSettings": [],
    "shellcheck.customArgs": [],
    "shellcheck.enable": true,
    "shellcheck.ignorePatterns": {},
    "shellcheck.run": "onSave",
    "telemetry.telemetryLevel": "off",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.persistentSessionExperimentalSerializer": false,
    "terminal.integrated.persistentSessionScrollback": 100,
    "vsicons.projectDetection.autoReload": true,
    "window.autoDetectHighContrast": false,
    "window.newWindowDimensions": "fullscreen",
    "window.restoreFullscreen": true,
    "window.titleBarStyle": "native",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.labelFormat": "default",
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "vscode-icons",
    "workbench.list.smoothScrolling": true,
    "workbench.startupEditor": "newUntitledFile",
    "files.associations": {
        "Dockerfile-*": "dockerfile",
        "buildx*": "dockerfile",
        "*.json.*": "json",
        "*.ps1.*": "powershell",
        "**/azure-pipelines*.yml": "azure-pipelines",
        "*.yml.*": "yaml",
        "*.yaml.*": "yaml",
        "terragrunt.hcl": "terraform",
        "*.tfvars": "terraform",
        "*.tf": "terraform",
        "*.tf_": "terraform",
        "*.tf.*": "terraform",
        "*.ctmpl": "go",
        "*.hcl": "go",
        ".terraformrc": "ini"
    },
    "shellcheck.exclude": [
        "SC2091"
    ],
    "spellright.language": [
        "en"
    ],
    "spellright.documentTypes": [],
    "files.watcherExclude": {
        "**/.terraform/**": true,
        "**/.terragrunt-cache/**": true
    },
    "[terraform]": {
        "editor.formatOnSave": true
    },
    "terraform.languageServer": {
        "external": true,
        "args": [
            "serve"
        ]
    },
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        }
    },
    "scm.showActionButton": false
}
