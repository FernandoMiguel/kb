{
    "breadcrumbs.enabled": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.folding": false,
    "editor.foldingStrategy": "indentation",
    "editor.fontSize": 14,
    "editor.formatOnSave": true,
    "editor.glyphMargin": false,
    "editor.insertSpaces": false,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.showFoldingControls": "always",
    "editor.tabCompletion": "on",
    "editor.trimAutoWhitespace": true,
    "editor.wordWrap": "on",
    "explorer.compactFolders": false,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.alwaysSignOff": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.detectSubmodulesLimit": 100,
    "git.enableCommitSigning": true,
    "git.enableSmartCommit": true,
    "git.fetchOnPull": true,
    "hadolint.hadolintPath": "/usr/local/bin/hadolint",
    "json.format.enable": true,
    "markdown.preview.breaks": true,
    "markdown.preview.scrollEditorWithPreview": true,
    "scm.alwaysShowProviders": true,
    "scm.providers.visible": 0,
    "search.useIgnoreFiles": false,
    "shellcheck.customArgs": [],
    "shellcheck.enable": true,
    "shellcheck.executablePath": "/usr/local/bin/shellcheck",
    "shellcheck.ignorePatterns": {},
    "shellcheck.run": "onSave",
    "shellcheck.useWSL": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.external.osxExec": "iTerm.app",
    "terraform.codelens.enabled": true,
    "terraform.lintPath": "/usr/local/bin/tflint",
    "terraform.path": "/usr/local/bin/terraform",
    "terraform.telemetry.enabled": false,
    "vsicons.presets.jsonOfficial": true,
    "vsicons.projectDetection.autoReload": true,
    "window.nativeTabs": true,
    "window.newWindowDimensions": "fullscreen",
    "window.restoreFullscreen": true,
    "window.restoreWindows": "all",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 0,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.labelFormat": "default",
    "workbench.enableExperiments": false,
    "workbench.startupEditor": "newUntitledFile",
    "files.associations": {
        "Dockerfile-*": "dockerfile",
        "buildx*": "dockerfile",
        "*.yml.*": "yaml",
        "terragrunt.hcl": "terraform",
        "*.tfvars": "terraform",
        "*.tf": "terraform",
        "*.tf_": "terraform",
        "*.tf.override": "terraform",
        "*.ctmpl": "gogo"
    },
    "shellcheck.exclude": [
        "SC2091"
    ],
    "spellright.language": [
        "en"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex"
    ],
    "[terraform]": {},
    "terraform.format": {
        "enable": true,
        "formatOnSave": true
    },
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            ".terraform/**/*",
            "**/.terraform/**/*",
            ".terragrunt-cache/**/*",
            "**/.terragrunt-cache/**/*"
        ]
    },
    "files.watcherExclude": {
        "**/.terraform/**": true,
        "**/.terragrunt-cache/**": true
    },
    "terraform.languageServer": {
        "enabled": true,
        "args": []
    },
    "workbench.iconTheme": "vscode-icons",
}
