{
    "breadcrumbs.enabled": false,
    "editor.folding": false,
    "editor.foldingStrategy": "indentation",
    "editor.fontSize": 14,
    "editor.formatOnSave": true,
    "editor.glyphMargin": false,
    "editor.insertSpaces": false,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.showFoldingControls": "always",
    "editor.tabCompletion": "on",
    "editor.trimAutoWhitespace": true,
    "editor.wordWrap": "on",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.eol": "\n",
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.alwaysSignOff": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.detectSubmodulesLimit": 100,
    "git.enableCommitSigning": true,
    "git.enableSmartCommit": true,
    "git.fetchOnPull": true,
    "json.format.enable": true,
    "markdown.preview.scrollEditorWithPreview": true,
    "scm.alwaysShowProviders": true,
    "scm.providers.visible": 0,
    "shellcheck.customArgs": [],
    "shellcheck.enable": true,
    "shellcheck.exclude": [
        "SC2091"
    ],
    "search.useIgnoreFiles": false,
    "shellcheck.executablePath": "/usr/local/bin/shellcheck",
    "shellcheck.ignorePatterns": {},
    "shellcheck.run": "onSave",
    "shellcheck.useWSL": false,
    "sync.autoUpload": true,
    "sync.gist": "",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terraform.telemetry.enabled": false,
    "vsicons.projectDetection.autoReload": true,
    "window.newWindowDimensions": "fullscreen",
    "window.restoreFullscreen": true,
    "window.restoreWindows": "all",
    "window.zoomLevel": 0,
    "workbench.editor.labelFormat": "default",
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.feedback.visible": false,
    "files.associations": {
        "Dockerfile-*": "dockerfile",
        "*.yml.*": "yaml",
        "*.tpl": "shellscript",
        "*.tf": "terraform",
        "*.tf_": "terraform",
        "*.tf.override": "terraform"
    },
    "spellright.language": [
        "en"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex"
    ],
    "terraform.format": {
        "enable": true,
        "formatOnSave": true
    },
    "terraform.path": "/usr/local/bin/terraform",
    "terraform.lintPath": "/usr/local/bin/tflint",
    "terraform.codelens.enabled": true,
    "terraform.indexing": {
        "enabled": true,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            ".terraform/**/*",
            "**/.terraform/**/*"
        ]
    },
}