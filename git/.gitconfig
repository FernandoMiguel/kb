[user]
	name = Fernando Miguel
	email = github@FernandoMiguel.net
[core]
	editor = nano -w
	excludesfile = /Users/fernando/.gitignore_global
[color]
	ui = auto
[credential]
	helper = osxkeychain
[commit]
	template = /Users/fernando/.stCommitMsg
	gpgSign = true
[status]
	submoduleSummary = true
[url "https://github.com/"]
	insteadOf = git@github.com:
[url "https://"]
	insteadOf = git://
[gpg]
	program = /usr/local/bin/krgpg
[tag]
	forceSignAnnotated = true
